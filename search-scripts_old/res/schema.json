{
  "settings": {
    "number_of_shards": 12,
    "refresh_interval": "60s",
    "number_of_replicas": 1,
    "index.max_result_window": 500000,
    "similarity": {
      "scripted_tf": {
        "type": "scripted",
        "script": {
          "source": " return query.boost;"
        }
      },
      "scripted_normalized_tf": {
        "type": "scripted",
        "script": {
          "source": "double norm = 1/Math.sqrt(doc.length); return query.boost * norm;"
        }
      }
    },
    "analysis": {
      "analyzer": {
        "graph_highlight_analyzer": {
          "tokenizer": "default_tokenizer",
          "filter": [
            "lowercase",
            "stemmer_protection",
            "light_english_stemmer",
            "synonym_graph"
          ]
        },
        "default": {
          "tokenizer": "default_tokenizer",
          "filter": [
            "lowercase",
            "stemmer_protection",
            "light_english_stemmer",
            "synonym",
            "eng_stopwords",
            "custom_stopwords",
            "default_delimiter"
          ]
        },
        "wo_syn_analyzer": {
          "tokenizer": "default_tokenizer",
          "filter": [
            "lowercase",
            "wo_syn_index_delimiter",
            "stemmer_protection",
            "light_english_stemmer"
          ]
        },
        "synonyms_analyzer": {
          "tokenizer": "default_tokenizer",
          "filter": [
            "synonym"
          ]
        },
        "cleaner": {
          "tokenizer": "default_tokenizer",
          "filter": [
            "lowercase",
            "minimal_english_stemmer"
          ]
        },
        "default_search": {
          "tokenizer": "default_tokenizer",
          "filter": [
            "lowercase",
            "stemmer_protection",
            "light_english_stemmer",
            "search_synonym",
            "eng_stopwords",
            "custom_stopwords",
            "default_delimiter"
          ]
        }
      },
      "tokenizer": {
        "default_tokenizer": {
          "type": "pattern",
          "pattern": " |,|-|;|:|\\(|\\)|\\*|\\?|\\/|\\[|\\]|\\{|\\}|!|&|\"|\\'"
        }
      },
      "filter": {
        "index_multiplexer": {
          "type": "multiplexer",
          "filters": [
            "lowercase",
            "synonym",
            "default_delimiter",
            "english_stemmer"
          ]
        },
        "search_multiplexer": {
          "type": "multiplexer",
          "filters": [
            "lowercase",
            "wo_syn_index_delimiter",
            "english_stemmer",
            "synonym_graph"
          ]
        },
        "eng_stopwords": {
          "type": "stop",
          "stopwords": [
            "a",
            "an",
            "and",
            "are",
            "as",
            "at",
            "be",
            "but",
            "by",
            "for",
            "if",
            "into",
            "is",
            "no",
            "not",
            "of",
            "on",
            "or",
            "such",
            "that",
            "the",
            "their",
            "then",
            "there",
            "these",
            "they",
            "this",
            "to",
            "will",
            "with",
            "in"
          ]
        },
        "custom_stopwords": {
          "type": "stop",
          "stopwords": [
            "senior",
            "professional",
            "specialist",
            "expert",
            "sr",
